<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aide_gainageFragment">

    <ImageView
        android:id="@+id/image_gainage"
        android:layout_width="311dp"
        android:layout_height="135dp"
        android:layout_marginBottom="233dp"
        android:layout_marginEnd="36dp"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="36dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="164dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gainage" />


    <Button
        android:id="@+id/btn_valider_gainage"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginBottom="63dp"
        android:layout_marginEnd="152dp"
        android:layout_marginLeft="152dp"
        android:layout_marginRight="152dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/valider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_gainage"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>